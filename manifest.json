{
    "manifest_version": 2,
    "name": "Trello Squadification",
    "version": "0.0.2",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "64": "icons/icon64.png",
        "128": "icons/icon128-96.png"
    },
    "description": "Distributed squadification for Trello",
    "content_scripts": [{
        "css": ["styles.css"],
        "js": ["jquery.js", "Chart.js", "content.js"],
        "matches": ["https://trello.com/*"]
    }]
}
